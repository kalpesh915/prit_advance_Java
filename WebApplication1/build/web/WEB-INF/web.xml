<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.1" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_1.xsd">
    <servlet>
        <servlet-name>Process1</servlet-name>
        <servlet-class>Process1</servlet-class>
        
        <init-param>
            <param-name>city</param-name>
            <param-value>Rajkot</param-value>
        </init-param>
         <init-param>
            <param-name>drivername</param-name>
            <param-value>org.derby.jdbc.driver</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Process1</servlet-name>
        <url-pattern>/process1</url-pattern>
    </servlet-mapping>
</web-app>
